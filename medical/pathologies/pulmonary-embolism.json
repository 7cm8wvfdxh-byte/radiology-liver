{
  "id": "pulmonary-embolism",
  "title": "Pulmoner Emboli",
  "core": {
    "definition": "Pulmoner arterlerin trombüs ile akut obstrüksiyonudur; sıklıkla alt ekstremite derin ven trombozundan kaynaklanır.",
    "etiology": [
      "Derin ven trombozu (en sık kaynak)",
      "Pelvik ven trombozu",
      "Nadir: tümör embolisi, yağ embolisi, septik emboli"
    ],
    "risk_factors": [
      "Yakın cerrahi/immobilizasyon",
      "Malignite",
      "Gebelik/postpartum",
      "Östrojen içeren ilaçlar",
      "Önceki VTE öyküsü",
      "Trombofili",
      "Uzun seyahat/immobilizasyon",
      "Obezite"
    ],
    "pathophysiology_short": [
      "Pulmoner vasküler direnç artışı → RV afterload ↑",
      "RV dilatasyonu/iskemi → şok riski",
      "V/Q uyumsuzluğu → hipoksemi"
    ]
  },
  "clinical": {
    "symptoms": [
      "Ani dispne",
      "Plevritik göğüs ağrısı",
