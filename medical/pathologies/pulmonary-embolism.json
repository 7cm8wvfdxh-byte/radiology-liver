  "radiology": {
    "overview": "Klinik olasılık + D-dimer yaklaşımı sonrası, tanıda ana görüntüleme genellikle CTPA’dır. Hemodinamik instabil hastada bedside EKO/RV strain ve kompresyon USG yardımcı olabilir.",
    "modalities": {
      "CT": {
        "summary": "CT Pulmoner Anjiyo (CTPA) tanıda ana yöntem; RV/LV oranı ile sağ kalp yüklenmesi değerlendirilebilir.",
        "key_findings": [
          "Pulmoner arterde dolum defekti",
          "Saddle embolus",
          "RV/LV oran artışı",
          "Pulmoner infarkt: periferik kama şeklinde konsolidasyon"
        ],
        "supportive_findings": [
          "Plevral efüzyon (bazen)",
          "Atelektazi (nonspesifik)"
        ],
        "complications": [
          "Pulmoner infarkt",
          "Sağ kalp yetmezliği bulguları"
        ],
        "pitfalls": [
          "Yetersiz kontrast opasifikasyonu",
          "Hareket artefaktı",
          "Subsegmental dolum defektlerinde yanlış pozitif/negatif"
        ],
        "ddx_imaging": [
          "Pulmoner arterit/vasculitis",
          "Pulmoner arter tümörü (çok nadir)",
          "Mukus tıkacı (hava yolu; arter değil)"
        ],
        "how_to_distinguish": [
          {
            "name": "PE vs artefakt",
            "how_to_distinguish": "Gerçek dolum defekti damar lümeni içinde, çoklu kesitte izlenir; artefaktlar genellikle tek kesit/kenar uyumsuzdur."
          }
        ]
      },
      "USG": {
        "summary": "Alt ekstremite kompresyon venöz USG ile DVT gösterilmesi PE lehine güçlü dolaylı kanıttır; acilde hızlıdır.",
        "key_findings": [
          "Kompresyona dirençli ven (DVT)",
          "Ven içinde ekojen trombüs"
        ],
        "supportive_findings": [],
        "complications": [],
        "pitfalls": [
          "Distal DVT kaçabilir",
          "İyi teknik ve deneyim gerekir"
        ],
        "ddx_imaging": [
          "Baker kisti/hematom",
          "Sellülit (klinik ayırım)"
        ],
        "how_to_distinguish": []
      },
      "MR_MRCP": {
        "summary": "MR pulmoner anjiyo seçilmiş olguda alternatif olabilir (kontrast/CT kısıtlılığı). Rutin ilk seçenek değildir.",
        "key_findings": [
          "Pulmoner arterde sinyal/dolum defekti (sekansa bağlı)"
        ],
        "supportive_findings": [],
        "complications": [],
        "pitfalls": [
          "Erişilebilirlik ve süre",
          "Hareket/nefes artefaktı"
        ],
        "ddx_imaging": [],
        "how_to_distinguish": []
      }
    }
  },
