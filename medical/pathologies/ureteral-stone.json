{
  "id": "ureteral-stone",
  "title": "Üreter Taşı (Renal Kolik)",
  "core": {
    "definition": "Üreter lümeninde taşın obstrüksiyon yapması sonucu akut kolik tarzı ağrı ve/veya obstrüktif üropati tablosudur.",
    "etiology": [
      "Kalsiyum oksalat/fosfat taşları (en sık)",
      "Ürik asit taşları",
      "Strüvit (enfeksiyonla ilişkili)",
      "Sistin (nadir)"
    ],
    "risk_factors": [
      "Düşük sıvı alımı/dehidratasyon",
      "Önceki taş öyküsü",
      "Yüksek tuz/oksalat alımı",
      "Hiperparatiroidi",
      "Gut/hiperürisemi",
      "Enfeksiyon (strüvit)",
      "Obezite/metabolik sendrom"
    ],
    "pathophysiology_short": [
      "Obstrüksiyon → toplayıcı sistem basıncı ↑ → ağrı",
      "Üreter peristaltizmi artışı (kolik)",
      "Uzamış obstrüksiyon → hidronefroz/renal fonksiyon etkilenebilir"
    ]
  },
  "clinical": {
    "symptoms": [
      "Flank ağrısı (kolik), kasığa yayılım",
      "Bulantı-kusma",
      "Makroskopik/mikroskopik hematüri",
      "Dizüri/üriner irritatif semptomlar (distal taşta)"
    ],
    "physical_exam": [
      "Kostovertebral açı hassasiyeti",
      "Ateş varsa enfeksiyon eşlik edebilir"
    ],
    "scores_classifications": [
      "Taş boyutu ve lokalizasyon (spontan düşme olasılığı için pratik sınıflama)",
      "Obstrüksiyon derecesi (hidronefroz)"
    ],
    "exam": {
      "title": "Fizik muayene",
      "items": []
    },
    "red_flags": {
      "title": "Acil kırmızı bayraklar",
      "items": []
    },
    "scores": {
      "title": "Skor / sınıflama",
      "items": []
    }
  },
  "labs": {
    "patterns": [
      "İdrar tahlili: hematüri (sık)",
      "Lökositoz/CRP (enfeksiyon varsa artabilir)",
      "Kreatinin (obstrüksiyon/tek böbrek vb. durumlarda)"
    ],
    "red_flags": [
      "Ateş/sepsis bulguları (obstrüksiyon + enfeksiyon acildir)",
      "Tek böbrek/renal yetmezlik",
      "Kontrolsüz ağrı",
      "Gebelik"
    ],
    "key_labs": {
      "title": "Laboratuvar (key labs)",
      "items": []
    },
    "notes": {
      "title": "Laboratuvar notları",
      "items": []
    }
  },
  "radiology": {
    "overview": "Non-kontrast düşük doz BT (KUB) erişkinde en duyarlı yöntemdir. USG özellikle gebelik/çocukta ve hidronefroz değerlendirmesinde ilk basamak olabilir.",
    "modalities": {
      "CT": {
        "summary": "Non-kontrast BT KUB: taşın yeri/boyu, hidronefroz ve alternatif tanılar değerlendirilir.",
        "key_findings": [
          "Üreter boyunca hiperdens taş",
          "Proksimal dilatasyon/hidronefroz",
          "Periüreterik/perirenal yağ planlarında stranding"
        ],
        "supportive_findings": [
          "Üreteral duvar kalınlaşması/ödem",
          "İdrar ekstravazasyonu şüphesi (nadir)"
        ],
        "complications": [
          "Obstrüktif üropati",
          "Perinefrik koleksiyon (nadir, rüptür)"
        ],
        "pitfalls": [
          "Flebolit ile karışma",
          "Çok küçük taşların atlanması (kesit kalınlığı/artefakt)"
        ],
        "ddx_imaging": [
          "Flebolit",
          "Appendisit/divertikülit (alt kadran ağrısında)",
          "Pyelonefrit (klinik + kontrastlı inceleme gerekebilir)"
        ],
        "how_to_distinguish": [
          {
            "name": "Taş vs flebolit",
            "how_to_distinguish": "Taş üreter trasesinde ve hidronefroz/stranding ile ilişkili olabilir; flebolitte genellikle 'comet tail' işareti görülebilir."
          }
        ]
      },
      "USG": {
        "summary": "Hidronefroz varlığı ve distal üreter/UVJ taşı bazı olgularda görülebilir. Gebelik ve çocukta önemlidir.",
        "key_findings": [
          "Hidronefroz/hidroüreter",
          "UVJ’de hiperekojen fokus + akustik gölge (görülebilirse)"
        ],
        "supportive_findings": [
          "Üreter jetinin azalması (Doppler, destekleyici)"
        ],
        "complications": [
          "İleri hidronefroz"
        ],
        "pitfalls": [
          "Üreterin tamamı görülemeyebilir",
          "Küçük taşlar kaçabilir"
        ],
        "ddx_imaging": [
          "Fizyolojik pelvikaliektazi (gebelik)",
          "Pyelonefrit (klinik korelasyon)"
        ],
        "how_to_distinguish": []
      },
      "MR_MRCP": {
        "summary": "MR ürografi seçilmiş olguda (özellikle gebelikte) alternatif olabilir; taşın kendisi yerine obstrüksiyon bulguları ön plandadır.",
        "key_findings": [
          "Hidroüreteronefroz",
          "Obstrüksiyon seviyesinde sinyal değişikliği (indirekt)"
        ],
        "supportive_findings": [],
        "complications": [],
        "pitfalls": [
          "Taş direkt görünmeyebilir",
          "Erişilebilirlik"
        ],
        "ddx_imaging": [],
        "how_to_distinguish": []
      }
    }
  },
  "management": {
    "triage": [
      "Ateş/sepsis + obstrüksiyon: acil ürolojik dekompresyon değerlendirmesi",
      "Stabil: ağrı kontrolü + spontan pasaj olasılığına göre izlem"
    ],
    "initial_approach": [
      "Analjezi ve hidrasyon",
      "Enfeksiyon şüphesi varsa antibiyotik değerlendirmesi",
      "Taş boyutu/lokalizasyonuna göre takip planı"
    ],
    "next_steps": [
      "Küçük taşlarda (özellikle distal) medikal ekspulsif tedavi düşünülebilir (klinik karar)",
      "Büyük taş/komplikasyon: üroloji girişimi (URS, stent vb.)"
    ],
